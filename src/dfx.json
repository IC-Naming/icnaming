{
  "canisters": {
    "registry": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package registry --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/registry.wasm -o target/wasm32-unknown-unknown/release/registry_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/registry_opt.wasm",
      "candid": "canisters/registry/src/registry.did"
    },
    "registrar": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package registrar --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/registrar.wasm -o target/wasm32-unknown-unknown/release/registrar_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/registrar_opt.wasm",
      "candid": "canisters/registrar/src/registrar.did"
    },
    "registrar_control_gateway": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package registrar_control_gateway --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/registrar_control_gateway.wasm -o target/wasm32-unknown-unknown/release/registrar_control_gateway_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/registrar_control_gateway_opt.wasm",
      "candid": "canisters/registrar_control_gateway/src/registrar_control_gateway.did"
    },
    "resolver": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package resolver --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/resolver.wasm -o target/wasm32-unknown-unknown/release/resolver_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/resolver_opt.wasm",
      "candid": "canisters/resolver/src/resolver.did"
    },
    "favorites": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package favorites --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/favorites.wasm -o target/wasm32-unknown-unknown/release/favorites_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/favorites_opt.wasm",
      "candid": "canisters/favorites/src/favorites.did"
    },
    "cycles_minting": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package cycles_minting --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/cycles_minting.wasm -o target/wasm32-unknown-unknown/release/cycles_minting_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/cycles_minting_opt.wasm",
      "candid": "canisters/cycles_minting/src/cycles_minting.did"
    },
    "ledger": {
      "type": "custom",
      "candid": "scripts/wasm/ledger.public.did",
      "wasm": "scripts/wasm/ledger-canister.wasm"
    },
    "icnaming_ledger": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package icnaming_ledger --release --no-default-features --features dev_canister",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/icnaming_ledger.wasm -o target/wasm32-unknown-unknown/release/icnaming_ledger_opt.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/icnaming_ledger_opt.wasm",
      "candid": "canisters/icnaming_ledger/src/icnaming_ledger.did"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}